
@{
}

@model IEnumerable<string>
<h2>Filter by category: </h2><br />
<a asp-controller="Home" asp-action="Index" asp-route-category=""><h4>View all Books</h4></a><br />
@foreach (string cat in Model)
{
        <a asp-controller="Home" asp-action="Index" asp-route-category="@cat"><h4>@cat</h4></a><br/>
}

